server {
    listen       80 default_server;
    server_name  nginx.imssyang.com web.imssyang.com;

    location / {
        root   html;
        index  index.html index.htm index.php;
    }

    location ^~ /.well-known/acme-challenge {
        root html/acme;
    }

    error_page  404              /404.html;
    error_page  500 502 503 504  /50x.html;
    location = /50x.html {
        root   html;
    }

