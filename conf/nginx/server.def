server {
    listen       443  ssl  default_server;
    server_name  nginx.imssyang.com;

    ssl_certificate      /opt/openssl/ssl/certs/letsencrypt/imssyang.com/fullchain.cer;
    ssl_certificate_key  /opt/openssl/ssl/certs/letsencrypt/imssyang.com/imssyang.com.key;

    ssl_session_cache    shared:SSL:1m;
    ssl_session_timeout  5m;

    ssl_ciphers  HIGH:!aNULL:!MD5;
    ssl_prefer_server_ciphers  on;

    location / {
        root   html;
        index  index.html index.htm;
        #proxy_pass http://127.0.0.1:4000;
    }
}

server {
    listen       80;
    server_name  nginx-demo.imssyang.com;

    location / {
        proxy_pass        http://192.168.5.151:31674;
        proxy_set_header  Host $host;
    }
}

